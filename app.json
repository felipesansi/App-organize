{
  "expo": {
    "name": "organizarApp",
    "slug": "organizarApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Icon.png",
    "scheme": "organizarapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#16233A"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ffreita_s.organizarApp", 
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Adaptive-Icon.png",
        "backgroundColor": "#16233A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ffreita_s.organizarApp"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "3d4f0059-daae-4689-b897-451f7616ea50"
      }
    }
  }
}
